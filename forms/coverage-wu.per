attributes
p01 = coverage_wu.ceid, downshift, queryclear, noupdate, required, default="";
    = skills_wu.ceid, downshift, queryclear, noentry, noupdate, color=reverse;
    = success_wu.ceid, downshift, queryclear, noentry, noupdate, color=reverse;
    = contacts_wu.c_ceid, downshift, queryclear, noentry, noupdate, color=reverse;
p02 = coverage_wu.part_name, queryclear, required, color=reverse,
      comments="Partner Name";
a1   = coverage_wu.vad=*vads.v_id, queryclear, default="",
      comments="VAD";
b1   = coverage_wu.hq=*states.s_id, upshift, autonext, queryclear,
      comments="HQ: State where partner is headquartered";
c2   = coverage_wu.part_type, upshift, include=("B","G","O","S"," "), autonext, required,
      comments="Segment: (B)lue Core, (G)rowth, (O)pportunistic, (S)trategic";
z   = coverage_wu.reseller, upshift, include=("R","E","N","H","V","X"), default="R", autonext,
      comments="Reseller Code: (R)eseller,(E)merging,(H)igh Value,(V)ad,(N)ot Reselling,(X)gone";
note1   = coverage_wu.note, wordwrap compress,
      comments="Internal (to Coverage Team) Notes regarding this Partner";
y   = coverage_wu.lead, upshift, include=("BA","IM","EM","IO",""), autonext, 
      comments="Technical Lead: BA, IM, EM, IO";
w   = coverage_wu.tier, autonext, include=(0,1,2,3),
      comments="Partner is part of this Tier 0-3";
p03 = coverage_wu.ssr=*reps.r_name, queryclear, default="",
      comments="IBM Channels Brand Rep";
p04im = coverage_wu.ctp1=*reps2.r_name, queryclear, default="",
      comments="IBM Channels IM TSS Rep (use a single space to indicate empty)";
p04em = coverage_wu.ctp2=*reps3.r_name, queryclear, default="",
      comments="IBM Channels ECM TSS Rep (use a single space to indicate empty)";
p04ba = coverage_wu.ctp3=*reps4.r_name, queryclear, default="",
      comments="IBM Channels BA TSS Rep (use a single space to indicate empty)";
p04s1 = coverage_wu.ctp4=*reps5.r_name, queryclear, default="",
      comments="IBM Channels IoT TSS Rep (use a single space to indicate empty)";
p04s2 = coverage_wu.ctp5=*reps6.r_name, queryclear, default="",
      comments="IBM Channels TSS Rep (use a single space to indicate empty)";
p05 = coverage_wu.url,
      comments="Partners Corporate URL";
p06 = coverage_wu.pwp, wordwrap compress,
      comments="Partners Partners World Profile URL";
p08 = coverage_wu.p_name,
      comments="Partner contact name for passing leads";
p08a = coverage_wu.p_title,
      comments="Partner contact title";
p09 = coverage_wu.p_phone,
      comments="Partner contact phone number (with area code)";
p10 = coverage_wu.p_email,
      comments="Partner contact corporate email address";
j   = coverage_wu.hunter, upshift, autonext,
      comments="Partner is a Hunter - going after white space";
p11 = coverage_wu.focus1, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p12 = coverage_wu.focus2, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p13 = coverage_wu.focus3, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p07 = coverage_wu.desc, wordwrap compress,
      comments="Description about the partner - use <p> to separate paragraphs";



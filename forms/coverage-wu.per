instructions
   delimiters "  ";


   coverage_wu master of skills_wu
   skills_wu master of success_wu
   success_wu master of contacts_wu


   after editadd editupdate of coverage_wu
      let ts1 = current
{
      if( p03 is null and p04im is null and p04em is null and p04ba is null and p04s1 is null and p04s2 is null and z="R" )then
         let z = "N"
      if( p03 = " " and p04im = " " and p04em = " " and p04ba = " " and p04s1 = " " and p04s2 = " " and z="R" )then
         let z = "N"
}

   before editadd editupdate of coverage_wu.updtime
      nextfield = p02

   after editadd editupdate of skills_wu
      let ts2 = current

   before editadd editupdate of skills_wu.updtime
      nextfield = u0


      end

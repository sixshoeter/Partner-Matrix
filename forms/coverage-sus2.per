instructions
   delimiters "  ";


   coverage_su master of skills_s2
   skills_s2 master of success_su
   success_su master of contacts_su


   after editadd editupdate of coverage_su
      let ts1 = current

   before editadd editupdate of coverage_su.updtime
      nextfield = p02

   after editadd editupdate of skills_s2
      let ts2 = current

   before editadd editupdate of skills_s2.updtime
      nextfield = u0


      end

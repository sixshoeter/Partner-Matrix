attributes
p01 = coverage_su.ceid, downshift, queryclear, noupdate, required, default="";
    = skills_s2.ceid, downshift, queryclear, noentry, noupdate, color=reverse;
    = success_su.ceid, downshift, queryclear, noentry, noupdate, color=reverse;
    = contacts_su.c_ceid, downshift, queryclear, noentry, noupdate, color=reverse;
p02 = coverage_su.part_name, queryclear, required, color=reverse,
      comments="Partner Name";
a1   = coverage_su.vad=*vads.v_id, queryclear, default="",
      comments="VAD";
b1   = coverage_su.hq=*states.s_id, upshift, autonext, queryclear,
      comments="HQ: State where partner is headquartered";
c2   = coverage_su.part_type, upshift, include=("B","G","O","S"," "), autonext, required,
      comments="Segment: (B)lue Core, (G)rowth, (O)pportunistic, (S)trategic";
z   = coverage_su.reseller, upshift, include=("R","E","N","H","V"), default="R", autonext,
      comments="Reseller Code: (R)eseller, (E)merging, (H)igh Value, (V)ad, (N)ot Reselling";
note1   = coverage_su.note, wordwrap compress,
      comments="Internal (to Coverage Team) Notes regarding this Partner";
y   = coverage_su.lead, upshift, include=("S1","S2","S3","S4","S5"," ",""), autonext, 
      comments="Technical Lead: BA, IM, EM ";
w   = coverage_su.tier, autonext, include=(0,1,2,3),
      comments="Partner is part of this Tier 0-3";
p03 = coverage_su.ssr=*reps.r_name, queryclear, default="",
      comments="IBM Channels Brand Rep";
p04ap = coverage_su.ctp1=*reps2.r_name, queryclear, default="",
      comments="IBM Channels TSS Rep (use single space to indicate empty)";
p04int = coverage_su.ctp2=*reps3.r_name, queryclear, default="",
      comments="IBM Channels TSS Rep (use single space to indicate empty)";
p04sp = coverage_su.ctp3=*reps4.r_name, queryclear, default="",
      comments="IBM Channels TSS Rep (use single space to indicate empty)";
p04sm = coverage_su.ctp4=*reps5.r_name, queryclear, default="",
      comments="IBM Channels TSS Rep (use single space to indicate empty)";
p04mf = coverage_su.ctp5=*reps6.r_name, queryclear, default="",
      comments="IBM Channels TSS Rep (use single space to indicate empty)";
p05 = coverage_su.url,
      comments="Partners Corporate URL";
p06 = coverage_su.pwp, wordwrap compress,
      comments="Partners Partners World Profile URL";
p08 = coverage_su.p_name,
      comments="Partner contact name for passing leads";
p08a = coverage_su.p_title,
      comments="Partner contact title";
p09 = coverage_su.p_phone,
      comments="Partner contact phone number (with area code)";
p10 = coverage_su.p_email,
      comments="Partner contact corporate email address";
p11 = coverage_su.focus1, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p12 = coverage_su.focus2, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p13 = coverage_su.focus3, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p07 = coverage_su.desc, wordwrap compress,
      comments="Description about the partner - use <p> to separate paragraphs";



instructions
   delimiters "  ";


   coverage_es master of skills_es
   skills_es master of success_es
   success_es master of contacts_es


   after editadd editupdate of coverage_es
      let ts1 = current
      if( p03 is null and p04im is null and p04em is null and p04ba is null and z="R" )then
         let z = "N"
      if( p03 = " " and p04im = " " and p04em = " " and p04ba = " " and z="R" )then
         let z = "N"

   before editadd editupdate of coverage_es.updtime
      nextfield = p02

   after editadd editupdate of skills_es
      let ts2 = current

   before editadd editupdate of skills_es.updtime
      nextfield = u0


      end

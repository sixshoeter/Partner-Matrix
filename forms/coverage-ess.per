attributes
p01 = coverage_es.ceid, downshift, queryclear, noupdate, required, default="";
    = skills_es.ceid, downshift, queryclear, noentry, noupdate, color=reverse;
    = success_es.ceid, downshift, queryclear, noentry, noupdate, color=reverse;
    = contacts_es.c_ceid, downshift, queryclear, noentry, noupdate, color=reverse;
p02 = coverage_es.part_name, queryclear, required, color=reverse,
      comments="Partner Name";
a1   = coverage_es.vad=*vads.v_id, queryclear, default="",
      comments="VAD";
b1   = coverage_es.hq=*states.s_id, upshift, autonext, queryclear,
      comments="HQ: State where partner is headquartered";
c2   = coverage_es.part_type, upshift, include=("B","G","O","S"," "), autonext, required,
      comments="Segment: (B)lue Core, (G)rowth, (O)pportunistic, (S)trategic";
z   = coverage_es.reseller, upshift, include=("R","E","N","H","V"), default="R", autonext,
      comments="Reseller Code: (R)eseller, (E)merging, (H)igh Value, (V)ad, (N)ot Reselling";
note1   = coverage_es.note, wordwrap compress,
      comments="Internal (to Coverage Team) Notes regarding this Partner";
y   = coverage_es.lead, upshift, include=("BA","IM","EM","IO"," ",""), autonext, 
      comments="Technical Lead: BA, IM, EM ";
w   = coverage_es.tier, autonext, include=(0,1,2,3),
      comments="Partner is part of this Tier 0-3";
p03 = coverage_es.ssr=*reps.r_name, queryclear, default="",
      comments="IBM Channels Brand Rep";
p04im = coverage_es.ctp1=*reps2.r_name, queryclear, default="",
      comments="IBM Channels IM TSS Rep (use a single space to indicate empty)";
p04em = coverage_es.ctp2=*reps3.r_name, queryclear, default="",
      comments="IBM Channels ECM TSS Rep (use a single space to indicate empty)";
p04ba = coverage_es.ctp3=*reps4.r_name, queryclear, default="",
      comments="IBM Channels BA TSS Rep (use a single space to indicate empty)";
p05 = coverage_es.url,
      comments="Partners Corporate URL";
p06 = coverage_es.pwp, wordwrap compress,
      comments="Partners Partners World Profile URL";
p08 = coverage_es.p_name,
      comments="Partner contact name for passing leads";
p08a = coverage_es.p_title,
      comments="Partner contact title";
p09 = coverage_es.p_phone,
      comments="Partner contact phone number (with area code)";
p10 = coverage_es.p_email,
      comments="Partner contact corporate email address";
j   = coverage_es.hunter, upshift, autonext,
      comments="Partner is a Hunter - going after white space";
p11 = coverage_es.focus1, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p12 = coverage_es.focus2, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p13 = coverage_es.focus3, 
      comments="Other focus areas of partner (e.g., Mobility, etc)";
p07 = coverage_es.desc, wordwrap compress,
      comments="Description about the partner - use <p> to separate paragraphs";



function AddPlus( inStr )
define
   inStr char(100),
   i,l smallint

   let l=length(inStr)

   for i=1 to l
      if( inStr[i]=" " )then
         let inStr[i]="+"
      end if
   end for

   return inStr

end function  #AddPlus

